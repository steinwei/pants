{
  "name": "@pants/ui",
  "private": true,
  "version": "0.0.1",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.types.json --composite false",
    "build:prod": "pnpm build && pnpm build:types",
    "preview": "vite preview",
    "gen-version": "node ./scripts/gen_version.cjs.js",
    "test:unit": "vitest --environment jsdom"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/node": "^18.11.7",
    "@unocss/preset-mini": "^0.45.30",
    "@unocss/preset-uno": "^0.45.30",
    "@unocss/vite": "^0.45.30",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vueuse/core": "^9.4.0",
    "@vue/test-utils": "^2.2.0",
    "@vue/tsconfig": "^0.1.3",
    "typescript": "^4.6.4",
    "unocss": "^0.45.30",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
  }
}
